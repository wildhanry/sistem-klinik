# Example Environment Variables for Render
# Copy these to Render Dashboard > Environment tab
# DO NOT commit actual credentials to GitHub!

APP_NAME="Sistem Klinik"
APP_ENV=production
APP_DEBUG=false
APP_KEY=base64:YOUR_APP_KEY_HERE

# MySQL Database (Railway/PlanetScale)
DB_CONNECTION=mysql
DB_HOST=YOUR_MYSQL_HOST_HERE
DB_PORT=3306
DB_DATABASE=klinik_db
DB_USERNAME=YOUR_USERNAME_HERE
DB_PASSWORD=YOUR_PASSWORD_HERE

# Cache & Session
SESSION_DRIVER=database
CACHE_STORE=database

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=error
